{
    "name": "ebidtech/config-loader",
    "type": "library",
    "description": "Symfony 2 config component extra loaders",
    "keywords": ["symfony", "php", "library", "config", "loader", "yaml", "json"],
    "homepage": "http://github.com/ebidtech/config-loader",
    "license": "MIT",
    "authors": [
        {
            "name": "Ebidtech",
            "email": "developers@ebidtech.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php":            ">=5.4.0",
        "symfony/config": "~2.2",
        "symfony/yaml":   "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit":           "~5.0",
        "squizlabs/php_codesniffer": "~1.5",
        "phpmd/phpmd":               "~1.5",
        "satooshi/php-coveralls":    "~1.0",
        "mikey179/vfsStream":        "~1.2"
    },
    "autoload": {
        "psr-0": { "EBT\\ConfigLoader": "src/" }
    }
}
